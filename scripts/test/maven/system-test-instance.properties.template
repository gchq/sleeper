########################################################################################

# Set any instance or system test properties that you wish to override for all deployments here.
# Test runs will use a copy of this file with the same name but without `.template` on the end.
# Please do not edit the template. If you do not create the copy it will be created automatically.

# The minimum value of integers generated randomly during random row generation
sleeper.systemtest.random.int.min=0

# The maximum value of integers generated randomly during random row generation
sleeper.systemtest.random.int.max=100000000

# The minimum value of longs generated randomly during random row generation
sleeper.systemtest.random.long.min=0

# The maximum value of longs generated randomly during random row generation
sleeper.systemtest.random.long.max=10000000000

# The length of strings generated randomly during random row generation
sleeper.systemtest.random.string.length=10

# The length of byte arrays generated randomly during random row generation
sleeper.systemtest.random.bytearray.length=10

# The maximum number of entries in maps generated randomly during random row generation
# (the number of entries in the map will range randomly from 0 to this number)
sleeper.systemtest.random.map.length=10

# The maximum number of entries in lists generated randomly during random row generation
# (the number of entries in the list will range randomly from 0 to this number)
sleeper.systemtest.random.list.length=10
